<div class="container">
  <h3 id="tagline">We hope you find the lunch you seek at a sustainable price.</h3>
  <hr>
  <div class="col-sm-6">
    <br>
    <ul>
      <h2> Lunch Questions... </h2>
      {{#each model.questions as |question|}}
        <hr>
        {{question-tile question=question update="update"}}
        <hr>
      {{/each}}
    </ul>
    <br>
  </div>
  <div class="col-sm-5 col-sm-offset-1">
    {{new-question saveQuestion2="saveQuestion3"}}
    <hr>
    <h2> Favorite Q&amp;A's:  </h2>
    <br>
    <ul>
      {{display-favorites}}
    </ul>
  </div>
</div>
